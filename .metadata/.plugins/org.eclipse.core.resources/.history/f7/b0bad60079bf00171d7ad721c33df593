package Iterator;

public class DinerMenuIterator implements Iterator{
	
	MenuItem[] items;
	int position = 0;
	
	

	public DinerMenuIterator(MenuItem[] items) {
		super();
		this.items = items;
	}

	@Override
	public boolean hasNext() {
		if(position >= items.length)
			return false;
		return true;
	}

	@Override
	public Object next() {
		// TODO Auto-generated method stub
		return null;
	}

}
